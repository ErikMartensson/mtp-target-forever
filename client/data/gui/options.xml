<?xml version="1.0" standalone="no" ?>
<!-- this is a mtp-target gui resource -->
<resource>
	<object class="CGuiFrame" name="optionsFrame">
		<alignment>eAlignCenterHorizontal|eAlignCenterVertical</alignment>
		<minSize>250,180</minSize>
		<position>0,50</position>
		<element>
			<object class="CGuiVBox" name="optionsVbox0">
				<alignment>eAlignExpandHorizontal|eAlignExpandVertical</alignment>
				<element>
					<object class="CGuiText" name="optionsTitle">
						<alignment>eAlignCenterHorizontal|eAlignCenterVertical</alignment>
						<string> Video Options </string>
					</object>
					<object class="CGuiSpacer" name="spacer1">
						<minSize>0,5</minSize>
					</object>
					<object class="CGuiHBox" name="resolutionBox">
						<alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
						<element>
							<object class="CGuiText" name="resolutionLabel">
								<alignment>eAlignLeft|eAlignCenterVertical</alignment>
								<string>Resolution: </string>
							</object>
							<object class="CGuiButton" name="bResolution">
								<minSize>120,0</minSize>
								<element>
									<object class="CGuiText" name="txtResolution">
										<string>800x600</string>
									</object>
								</element>
							</object>
						</element>
					</object>
					<object class="CGuiHBox" name="fullscreenBox">
						<alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
						<element>
							<object class="CGuiText" name="fullscreenLabel">
								<alignment>eAlignLeft|eAlignCenterVertical</alignment>
								<string>Fullscreen: </string>
							</object>
							<object class="CGuiButton" name="bFullscreen">
								<minSize>60,0</minSize>
								<element>
									<object class="CGuiText" name="txtFullscreen">
										<string>OFF</string>
									</object>
								</element>
							</object>
						</element>
					</object>
					<object class="CGuiHBox" name="vsyncBox">
						<alignment>eAlignExpandHorizontal|eAlignCenterVertical</alignment>
						<element>
							<object class="CGuiText" name="vsyncLabel">
								<alignment>eAlignLeft|eAlignCenterVertical</alignment>
								<string>VSync: </string>
							</object>
							<object class="CGuiButton" name="bVSync">
								<minSize>60,0</minSize>
								<element>
									<object class="CGuiText" name="txtVSync">
										<string>OFF</string>
									</object>
								</element>
							</object>
						</element>
					</object>
					<object class="CGuiSpacer" name="spacer3">
						<minSize>0,10</minSize>
					</object>
					<object class="CGuiHBox" name="buttonBox">
						<alignment>eAlignCenterHorizontal|eAlignBottom</alignment>
						<element>
							<object class="CGuiButton" name="bApply">
								<minSize>80,0</minSize>
								<element>
									<object class="CGuiText" name="txtApply">
										<string>Apply</string>
									</object>
								</element>
							</object>
							<object class="CGuiButton" name="bOptionsBack">
								<minSize>80,0</minSize>
								<element>
									<object class="CGuiText" name="txtBack">
										<string>Back</string>
									</object>
								</element>
							</object>
						</element>
					</object>
				</element>
			</object>
		</element>
	</object>
</resource>
